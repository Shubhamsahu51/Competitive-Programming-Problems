
class Solution{
public:
    //Function to find the days of buying and selling stock for max profit.
    vector<vector<int> > stockBuySell(vector<int> A, int n){
        int profit=0;
        vector<vector<int>>v;
        for (int i=1;i<n;i++) {
            vector<int>nums;
            if (A[i]>A[i-1]) {
                nums.push_back(A[i-1]);
                nums.push_back(A[i]);
            }
            v.push_back(nums);
            
            
        }
        return v;
    }
};
